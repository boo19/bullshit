import java.util.Scanner;
 
public class HZ {
	public static void main(String args[]){
		Scanner in = new Scanner (System.in);
		System.out.print("День недели" );
 		int day = in.nextInt();
 		in.close();
 		
 		switch(day){
 		case 1:
 			System.out.print("Понедельник");
 			break;
 		case 2:
 			System.out.print("Вторник");
 			break;
 		case 3:
 			System.out.print("Среда");
 			break;
 		case 4:
 			System.out.print("Четверг");
 			break;
 		case  5:
 			System.out.print("Пятница");
 			break;
 		case  6:
 			System.out.print("Суббота");
 			break;
 		case  7:
 			System.out.print("Воскресенье");
 			break;
 			default:
 				System.out.print("В неделе 7 дней");
 				break;
 		}
 		
	}


}
